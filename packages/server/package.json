{
  "name": "@shenanigan/server",
  "version": "0.0.4",
  "description": "Backend for Shenanigan Dapp",
  "main": "dist/index.js",
  "repository": "https://github.com/ShenaniganDApp/shenanigan-monorepo/packages/server.git",
  "author": "Shenanigan Team",
  "license": "GNU",
  "private": true,
  "scripts": {
    "serve": "nodemon -r dotenv/config ./dist dotenv_config_path=../../.env",
    "build": "babel src --extensions \".es6,.js,.es,.jsx,.mjs,.ts,.tsx\" --ignore *.spec.js --out-dir dist --copy-files --source-maps --verbose",
    "typecheck": "yarn build",
    "lintfix": "eslint --fix",
    "update-schema": "babel-node --extensions \".es6,.js,.es,.jsx,.mjs,.ts\" ./scripts/updateSchema.ts",
    "precommit": "yarn lint-staged"
  },
  "dependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "@babel/node": "7.8.4",
    "@babel/plugin-proposal-async-generator-functions": "7.8.3",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-export-default-from": "7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.8.3",
    "@babel/plugin-transform-async-to-generator": "7.8.3",
    "@babel/preset-env": "7.8.7",
    "@babel/preset-flow": "7.8.3",
    "@babel/preset-react": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@entria/graphql-mongo-helpers": "^0.0.1",
    "@entria/graphql-mongoose-loader": "^4.3.0",
    "@shenanigan/utils": "1.0.6",
    "babel-plugin-relay": "9.0.0",
    "dataloader": "^2.0.0",
    "graphql": "^14.7.0",
    "graphql-playground-middleware-koa": "^1.6.17",
    "graphql-relay": "^0.6.0",
    "graphql-relay-subscription": "^0.3.1",
    "graphql-request": "^3.3.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tools": "^7.0.2",
    "imgix-core-js": "^2.3.2",
    "jsonwebtoken": "^8.5.1",
    "kcors": "^2.2.2",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-graphql": "^0.8.0",
    "koa-graphql-batch": "^1.1.0",
    "koa-router": "^10.0.0",
    "mongoose": "^5.9.3",
    "subscriptions-transport-ws": "^0.9.18"
  },
  "resolutions": {}
}
