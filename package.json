{
	"name": "@shenanigan/root",
	"private": true,
	"version": "0.1.0",
	"license": "GPL-3.0",
	"engines": {
		"node": ">=12"
	},
	"scripts": {
		"app:start": "lerna run start --parallel --scope @shenanigan/app --include-dependencies",
		"android": "yarn relay && lerna run android --parallel --scope @shenanigan/app --include-dependencies",
		"ios": "yarn relay && lerna run ios --parallel --scope @shenanigan/app --include-dependencies",
		"relay": "lerna run relay --parallel --scope @shenanigan/app",
		"compile": "lerna run compile --parallel --scope @shenanigan/buidler ",
		"deploy": "lerna run deploy --parallel --scope @shenanigan/buidler ",
		"server:serve": "lerna run serve --parallel --scope @shenanigan/server --include-dependencies",
		"server:build": "lerna run build --parallel --scope @shenanigan/server --include-dependencies",
		"test": "lerna run test --parallel --",
		"update-schema": "lerna run update-schema --parallel --scope @shenanigan/server --include-dependencies",
		"chain": "cd packages/buidler && npx buidler node",
		"sidechain": "cd packages/buidler && npx buidler node --port 8546",
		"generate": "lerna run generate --parallel --scope @shenanigan/buidler ",
		"accounts": "cd packages/buidler && npx buidler accounts",
		"account": "cd packages/buidler && npx buidler account",
		"balance": "cd packages/buidler && npx buidler balance",
		"send": "cd packages/buidler && npx buidler send",
		"test:full": "yarn lint && yarn typecheck && yarn test",
		"clean": "lerna clean",
		"format": "prettier --ignore-path .gitignore --write \"{*,**/*}.{ts,tsx,js,jsx,json,yml,yaml,md}\"",
		"lint": "eslint --ignore-path .gitignore \"./packages/**/*.{ts,tsx,js,jsx}\"",
		"typecheck": "lerna run typecheck",
		"app": "yarn --cwd packages/app/",
		"buidler": "yarn --cwd packages/buidler/"
	},
	"workspaces": {
		"packages": [
			"packages/**"
		],
		"nohoist": []
	},
	"dependencies": {
		"react-native": "0.62.2"
	},
	"devDependencies": {
		"@babel/core": "^7.6.2",
		"@babel/runtime": "^7.6.2",
		"@nomiclabs/buidler": "^1.4.4",
		"@nomiclabs/buidler-ethers": "^2.0.0",
		"@nomiclabs/buidler-truffle5": "^1.3.4",
		"@nomiclabs/buidler-waffle": "^2.0.0",
		"@nomiclabs/buidler-web3": "^1.3.3",
		"@opengsn/gsn": "2.0.0-alpha.3-patch.2",
		"@react-native-community/eslint-config": "^0.0.5",
		"@types/bcryptjs": "^2.4.2",
		"@types/graphql-relay": "^0.4.11",
		"@types/jest": "^26.0.0",
		"@types/jsonwebtoken": "^8.3.8",
		"@types/mongoose": "^5.7.6",
		"@types/node": "^13.9.1",
		"@types/react": "^16.9.38",
		"@types/react-native": "^0.62.13",
		"@types/react-native-dotenv": "^0.2.0",
		"@types/react-relay": "^7.0.8",
		"@types/react-test-renderer": "^16.9.2",
		"@types/relay-runtime": "^9.1.5",
		"@typescript-eslint/eslint-plugin": "3.9.1",
		"@typescript-eslint/parser": "3.6.0",
		"babel-jest": "^24.9.0",
		"babel-plugin-relay": "^9.1.0",
		"chai": "^4.2.0",
		"dotenv": "^8.2.0",
		"eslint": "6.8.0",
		"eslint-config-airbnb-typescript": "8.0",
		"eslint-config-prettier": "6.11.0",
		"eslint-import-resolver-typescript": "2.2.1",
		"eslint-plugin-import": "2.22.0",
		"eslint-plugin-jest": "23.20.0",
		"eslint-plugin-jsx-a11y": "6.3.1",
		"eslint-plugin-react": "7.20.3",
		"eslint-plugin-react-hooks": "4.0.6",
		"eslint-plugin-simple-import-sort": "5.0.3",
		"ethereum-waffle": "^3.0.0",
		"ethers": "^5.0.0",
		"graphql": "^15.0.0",
		"graphql-cli": "^3.0.14",
		"jest": "^24.9.0",
		"lerna": "^3.22.1",
		"metro-react-native-babel-preset": "^0.58.0",
		"nodemon": "^2.0.2",
		"prettier": "^2.0.5",
		"qrcode-terminal": "^0.12.0",
		"react-test-renderer": "16.11.0",
		"relay-compiler": "^9.1.0",
		"relay-compiler-language-typescript": "^12.0.3",
		"relay-devtools": "^1.4.0",
		"ts-node": "^8.6.2",
		"typescript": "^3.8.3"
	}
}
